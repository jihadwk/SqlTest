<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
    <!-- 兼容IE8 -->
<meta http-equiv="X-UA-Compatible" content="IE=8" />
 <!-- 引入JQuery -->
<script type="text/javascript" src="${pageContext.request.contextPath}/jquery-easyui-1.4/jquery-1.11.1.min.js"></script>
